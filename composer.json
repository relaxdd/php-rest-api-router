{
  "name": "relaxdd/php-rest-api-router",
  "description": "php-rest-api-router",
  "license": "MIT",
  "keywords": [
    "rest-api"
  ],
  "type": "library",
  "authors": [
    {
      "name": "awenn2015",
      "email": "awenn2015@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
