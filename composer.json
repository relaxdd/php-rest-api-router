  "suggest": {
    "ext-xdebug": "^3.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test-coverage": "phpunit --coverage-clover clover.xml"
  },
  "minimum-stability": "stable"
}
